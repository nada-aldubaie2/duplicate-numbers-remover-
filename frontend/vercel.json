{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://your-django-app.onrender.com/api/$1",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ]
  }
 